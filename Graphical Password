    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        
        String sql="insert into reg1(username) values(?)";
        String user=jTextField1.getText();
        try{
        PreparedStatement ps=conn.prepareStatement(sql);
        ps.setString(1,user);
        ps.executeUpdate();
        JOptionPane.showMessageDialog(null,"Congratulation,username is created.");
        imagepassregistration imagepass=new imagepassregistration();
        imagepass.show();
        
       }
        
     catch(SQLException  e){
             if(e instanceof SQLIntegrityConstraintViolationException){
             JOptionPane.showMessageDialog(null,"username is already exit.create a new username.");
             }
             else{
               
             }
             
             System.out.println(e);
             
        }
    }                                        

    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {                                         
       System.exit(0);
    }                                        

