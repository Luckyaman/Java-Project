     JButton l1=new JButton(new ImageIcon("C:\\Users\\User\\Documents\\NetBeansProjects\\Registrationpart\\picture\\1.jpg"));
    
     l1.addActionListener( new ActionListener(){
         
         public void actionPerformed(ActionEvent e) {
             try {

			BufferedImage originalImage = ImageIO.read(new File(
					"C:\\Users\\User\\Documents\\NetBeansProjects\\Registrationpart\\picture\\1.jpg"));

			ImageIO.write(originalImage, "jpg", new File(
					"C:\\Users\\User\\Documents\\NetBeansProjects\\Registrationpart\\imagepassword\\1.jpg"));

		} catch (IOException er) {
			System.out.println(er.getMessage());
		}
             
             
             JOptionPane.showMessageDialog(null,"Image is saved");
         }
     });
